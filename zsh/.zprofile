eval "$(/opt/homebrew/bin/brew shellenv)"

# Shortcuts
alias c='clear'
alias ..='cd ..'
alias untar='tar -xvzf'
alias untarxz='tar -xvJf'
alias dotfiles='nvim ~/dotfiles'

# Python
alias vc='python3 -m venv ./venv'
alias va='source ./venv/bin/activate'

# Listing
alias ls="ls -p"
alias ll="ls -Alph"
alias lla="ls -alph"

